{
  "name": "awilix-koa",
  "version": "0.0.0-development",
  "description": "Awilix helpers for Koa",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "build": "rimraf lib && tsc -p tsconfig.build.json",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "lint": "tslint --project tsconfig.json --fix \"src/**/*.ts\" && prettier --write \"src/**/*.ts\"",
    "cover": "jest --coverage",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "precommit": "lint-staged && npm run build",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jeffijoe/awilix-koa.git"
  },
  "files": [
    "lib",
    "LICENSE.md",
    "README.md"
  ],
  "directories": {
    "lib": "lib"
  },
  "keywords": [],
  "author": "Jeff Hansen <jeff@jeffijoe.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jeffijoe/awilix-koa/issues"
  },
  "homepage": "https://github.com/jeffijoe/awilix-koa#readme",
  "dependencies": {
    "awilix-router-core": "^1.3.2",
    "koa-compose": "^4.0.0",
    "koa-router": "^7.3.0",
    "tslib": "^1.8.0"
  },
  "devDependencies": {
    "@semantic-release/condition-codeship": "^1.1.0",
    "@semantic-release/release-notes-generator": "^6.0.3",
    "@types/glob": "^5.0.34",
    "@types/jest": "^22.0.0",
    "@types/koa": "^2.0.43",
    "@types/koa-compose": "^3.2.2",
    "@types/koa-router": "^7.0.25",
    "@types/node": "^8.5.2",
    "@types/prettier": "^1.9.0",
    "@types/rimraf": "^2.0.2",
    "assert-request": "^1.0.6",
    "awilix": "^3.0.1",
    "babel-jest": "^22.0.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-latest": "^6.24.1",
    "coveralls": "^3.0.0",
    "husky": "^0.14.3",
    "jest": "^22.0.4",
    "koa": "^2.4.1",
    "lint-staged": "^6.0.0",
    "prettier": "^1.9.2",
    "rimraf": "^2.6.1",
    "semantic-release": "^11.0.2",
    "semantic-release-conventional-commits": "^1.2.0",
    "ts-jest": "^22.0.0",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.6.0",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.6.1"
  },
  "peerDependencies": {
    "awilix": "^3.0.0"
  },
  "lint-staged": {
    "*.ts": [
      "tslint --fix --project tsconfig.json",
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "mapCoverage": true,
    "testRegex": "(/__tests__/.*\\.(test|spec))\\.(ts|tsx|js)$",
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "modulePaths": [
      "src"
    ],
    "coverageDirectory": "<rootDir>/coverage",
    "transform": {
      "\\.(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      "\\.(js|jsx)": "babel-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "globals": {}
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "release": {
    "generateNotes": "@semantic-release/release-notes-generator",
    "analyzeCommits": {
      "path": "semantic-release-conventional-commits",
      "majorTypes": [
        "major",
        "breaking"
      ],
      "minorTypes": [
        "feat",
        "minor"
      ],
      "patchTypes": [
        "chore",
        "fix",
        "patch",
        "docs",
        "test",
        "style",
        "refactor"
      ],
      "mergePattern": "/^Merge pull request #(\\d+) from (.*)$/",
      "mergeCorrespondence": "['id', 'source']"
    }
  },
  "babel": {
    "presets": [
      "latest"
    ],
    "plugins": "transform-decorators-legacy"
  }
}
